{
	"manifest_version": 2,
	"name": "ChromeLogger",
	"version": "0.6",
	"icons": {
		"32": "icon32.png",
		"48": "icon48.png"
	},
	"minimum_chrome_version": "10.0",
	"description": "FireLogger for Chrome - a sexy server logger console",
	"devtools_page": "devtools.html",
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"experimental",
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}